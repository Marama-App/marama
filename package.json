{
  "name": "marama",
  "version": "0.2.0",
  "description": "Redux exercise (without React)",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "assert": "exit 0",
    "build": "webpack --config ./client/webpack.config.js",
    "dev": "nodemon --watch server & yarn build --watch",
    "e2e:sh": "cd ./test/e2e && codeceptjs shell",
    "e2e:debug": "codeceptjs run -c ./test/e2e/codecept.json --debug",
    "e2e": "codeceptjs run -c ./test/e2e/codecept.json",
    "e2e:assert": "yarn e2e && yarn assert",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "seed": "yarn knex seed:run",
    "lint": "eslint client server test --ignore-pattern 'bundle.js'",
    "lint:assert": "yarn lint && yarn assert",
    "postinstall": "yarn build && yarn knex migrate:latest",
    "test": "jest test/client test/server",
    "test:watch": "yarn test --watchAll",
    "test:assert": "yarn test && yarn assert",
    "start": "node server"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0"
    ]
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./test/client-setup.js"
    ]
  },
  "precommit": "lint:assert, test:assert, e2e:assert",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.1",
    "dotenv": "^5.0.1",
    "email-validator": "^1.1.1",
    "express": "^4.15.3",
    "express-jwt": "^5.3.1",
    "input-moment": "^0.4.0",
    "jsonwebtoken": "^8.2.0",
    "jwt-decode": "^2.2.0",
    "knex": "^0.14.4",
    "mailgun-js": "^0.16.0",
    "moment": "^2.18.1",
    "passport": "^0.4.0",
    "pg": "^7.4.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.1.0",
    "sodium": "^2.0.3",
    "superagent": "^3.8.1",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "codeceptjs-nightmare": "^1.1.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.1.0",
    "jsdom": "^11.2.0",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "raf": "^3.4.0",
    "redux-mock-store": "^1.3.0",
    "sqlite3": "^3.1.9",
    "supertest": "^3.0.0",
    "webpack-dev-server": "^2.11.1"
  }
}
